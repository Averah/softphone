{
    "name": "Softphone SIP",
    "version": "1.0",
    "manifest_version": 2,
    "description": "This extension allows to make phone calls ",
    "icons": {
      "16": "/icons/icon16.png",
      "128": "/icons/icon128.png"
    },
    "browser_action": {
      "default_title": "Softphone SIP",
      "default_popup": "index.html"
    },
    "permissions": [
      "storage",
      "audioCapture",
      "notifications",
      "unlimitedStorage",
      "http://*/*",
      "https://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    }
}
  