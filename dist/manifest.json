{
    "name": "Softphone SIP",
    "version": "1.0",
    "manifest_version": 2,
    "description": "This extension allows to make phone calls ",
    "icons": {
      "16": "/icons/icon16.png",
      "128": "/icons/icon128.png"
    },
    "browser_action": {
      "default_title": "Softphone SIP",
      "default_popup": "index.html"
    },
    "background_page": "background.html",
    "permissions": [
      "storage",
      "notifications",
      "unlimitedStorage",
      "http://*/*",
      "https://*/*",
      "background"
    ],
    "commands": {
      "makeCall": {
        "suggested_key": {
          "default": "Ctrl+Shift+Y",
          "mac": "Command+Shift+Y"
        },
        "description": "allows to make a call on dialer page"
      }
  }
}
  